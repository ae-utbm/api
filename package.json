{
  "name": "@ae_utbm/api",
  "version": "1.0.0",
  "private": true,
  "license": "GPL v3",
  "type": "commonjs",
  "scripts": {
    "up": "pnpm update && cd src/exported && pnpm update && cd ../..",
    "lint": "prettier --write . && eslint \"{src,tests}/**/*.ts\" --fix",
    "test": "pnpm workflow:init_db && jest --config jest.config.ts",
    "clean": "node clean.mjs",
    "db:create": "pnpm mikro-orm schema:fresh --run",
    "db:migrate": "pnpm mikro-orm migration:up",
    "db:seed": "pnpm mikro-orm seeder:run",
    "build": "pnpm clean && nest build",
    "start:debug": "nest start --watch --debug",
    "start:dev": "nest start --watch",
    "start:prod": "pnpm db:migrate && node dist/src/main.js",
    "workflow:lint": "eslint \"{src,tests}/**/*.ts\" --max-warnings=0",
    "workflow:test": "jest --config jest.config.ts --forceExit",
    "workflow:init_db": "pnpm ts-node tests/database.setup.ts",
    "workflow:setup_env": "pnpm ts-node tests/env.setup.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ae_utbm/typings": "file:src/exported",
    "@mikro-orm/cli": "^5.9.1",
    "@mikro-orm/core": "^5.9.1",
    "@mikro-orm/migrations": "^5.9.1",
    "@mikro-orm/nestjs": "^5.2.2",
    "@mikro-orm/postgresql": "^5.9.1",
    "@mikro-orm/reflection": "^5.9.1",
    "@mikro-orm/seeder": "^5.9.1",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.14",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "file-type": "^16.5.4",
    "jsonwebtoken": "^9.0.2",
    "nestjs-i18n": "^10.3.7",
    "nodemailer": "^6.9.7",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.6",
    "type-fest": "^4.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.8",
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.20",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/jest": "29.5.2",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/multer": "^1.4.9",
    "@types/node": "20.5.7",
    "@types/nodemailer": "^6.4.13",
    "@types/passport-jwt": "^3.0.12",
    "@types/supertest": "^2.0.15",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/src/mikro-orm.config.js"
    ]
  }
}
